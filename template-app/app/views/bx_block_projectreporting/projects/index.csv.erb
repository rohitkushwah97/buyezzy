<% headers = ["project_name", "start_date", "end_date","project_type","admin_id","manager_id",
  "co_manager_id","assessor_id"] %>
<%= CSV.generate_line headers %>
<% @projects.each do |project| %>
  <%= CSV.generate_line([project.name, project.start_date, project.end_date,project.project_type,project.account_id,project.manager_id,project.co_manager_id,"#{project.assessor_ids.join(", ")}"]) %>
<% end %>
